{
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/vercel_app.py"
        },
        {
            "src": "(.*)",
            "dest": "/vercel_app.py"
        }
    ]
}